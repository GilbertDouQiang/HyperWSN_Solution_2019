<Window x:Class="DeviceConfigTools2018.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeviceConfigTools2018"
        mc:Ignorable="d"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        Title="HyperWSN Device Config Tools" Height="800" Width="1500"
        WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" Closed="Window_Closed" FontFamily="Courier New" VerticalAlignment="Center" VerticalContentAlignment="Center" FontSize="14">
    <Window.Resources>
        <local:HyperMultiValueConverter x:Key="HyperMultiValueConverter" />
        <local:GatewayBindingModel x:Key="model" />
    </Window.Resources>
    <Grid>
        <Grid>
            <telerik:RadDocking HasDocumentHost="True" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch" Margin="0,10,0,-10">
                <telerik:RadSplitContainer Width="150">
                    <telerik:RadPaneGroup HorizontalAlignment="Stretch" VerticalAlignment="Stretch" FontSize="12">
                        <telerik:RadPane Header="Device Status">

                        </telerik:RadPane>
                    </telerik:RadPaneGroup>
                </telerik:RadSplitContainer>
                <telerik:RadDocking.DocumentHost>
                    <telerik:RadPaneGroup>
                        <telerik:RadDocumentPane Title="HyperWSN Device ">

                            <Border Padding="5 5 5 5">

                                <StackPanel x:Name="StackM1" Margin="0 0 0 0">

                                    <Grid Margin="0 5 0 0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="0.2*"/>
                                            <ColumnDefinition Width="1.0*"/>
                                            <ColumnDefinition Width="4.0*"/>
                                            <ColumnDefinition Width="0.2*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="0.2*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="0.2*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="0.2*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="0.2*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Text="Device List:  " Grid.Column="1" HorizontalAlignment="Right" Margin="0 0 3 0" VerticalAlignment="Center"/>
                                        <telerik:RadComboBox x:Name="cbDeviceList"  Grid.Column="2" Margin="0 0 0 0"/>
                                        <telerik:RadButton x:Name="btnConnectDevice" Click="btnConnectDevice_Click" Grid.Column="4" Content="Connect"  Margin="0,0,0,0" />
                                        <telerik:RadButton x:Name="btnRefersh"  Grid.Column="6" Content="Refresh" Click="btnRefersh_Click"  Margin="0 0 0 0" />
                                        <telerik:RadButton x:Name="btnStartCheck" Grid.Column="8"  Content="开始检查" Click="btnStartCheck_Click"  Margin="0 0 0 0" />
                                        <telerik:RadButton x:Name="btnStopCheck"  Grid.Column="10" Content="停止检查" Click="btnStopCheck_Click"   Margin="0 0 0 0" />
                                    </Grid>

                                    <Grid Margin="0 5 0 0" >

                                        <telerik:RadTabControl Padding="0, 0, 0, 0" x:Name="aRadTabControl" SelectedIndex="0" Background="{x:Null}" >

                                            <telerik:RadTabItem x:Name="RadTabItemOfCfg" Header=" Config ">
                                                <StackPanel x:Name="GatewayDetail">

                                                    <Grid Margin="0 15 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.01*"/>
                                                            <ColumnDefinition Width="0.99*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock  Grid.Column="1" Text="设备类型：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxDeviceType" Text="{Binding Name}" Grid.Column="2"  Grid.ColumnSpan="2" Margin="0 0 0 0" IsReadOnly="True" MaxLines="1" MaxLength="16"/>

                                                        <TextBlock   Grid.Column="5" Text="重启原因：" Margin="0,3,3,0" HorizontalAlignment="Right" TextDecorations="{x:Null}" />
                                                        <TextBox x:Name="tbxRstSrc" Text="{Binding RstSrc, StringFormat=\{0:X2\}}" Grid.Column="6" Grid.ColumnSpan="2" Margin="0,0,0,0" IsReadOnly="True" MaxLength="2" MaxLines="1" FontWeight="Bold"/>

                                                        <TextBlock  Grid.Column="9" Text="Primary MAC：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxPrimaryMac" Text="{Binding PrimaryMacS}" Grid.Column="10" Grid.ColumnSpan="2" Margin="0 0 0 0" IsReadOnly="True" MaxLines="1" MaxLength="11" FontWeight="Bold"/>

                                                        <Button x:Name="btnCopy" Content="Copy" Grid.Column="13" Click="btnCopy_Click" Margin="0,0,0,0"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock  Grid.Column="1" Text=" 协议版本号："  Margin="0,3,3,0" HorizontalAlignment="Right"/>
                                                        <TextBox x:Name="tbxProtocol" Text="{Binding Protocol, StringFormat=\{0:X2\}}" Grid.Column="2" Grid.ColumnSpan="2" Margin="0,0,0,0" IsReadOnly="True" MaxLines="1" MaxLength="2" FontWeight="Bold"/>

                                                        <TextBlock  Grid.Column="5" Text=" 软件主版本："  Margin="0,3,3,0" HorizontalAlignment="Right"/>
                                                        <TextBox x:Name="tbxSwRevisionM"    Text="{Binding SwRevisionS}" Grid.Column="6" Margin="0,0,5,0" IsReadOnly="True" MaxLines="1" MaxLength="5" FontWeight="Bold"/>
                                                        <TextBox x:Name="tbxNewSwRevisionM" Text="{Binding SwRevisionS}" Grid.Column="7" Margin="5,0,0,0" IsReadOnly="True" MaxLines="1" MaxLength="5" FontWeight="Bold" />

                                                        <TextBlock  Grid.Column="9" Text=" 软件次版本："  Margin="0,3,3,0" HorizontalAlignment="Right"/>
                                                        <TextBox x:Name="tbxSwRevisionS"    Text="{Binding HwRevisionS}" Margin="0,0,5,0" Grid.Column="10" IsReadOnly="True" MaxLines="1" MaxLength="5" FontWeight="Bold"/>
                                                        <TextBox x:Name="tbxNewSwRevisionS" Text="{Binding HwRevisionS}" Margin="5,0,0,0" Grid.Column="11" IsReadOnly="True" MaxLines="1" MaxLength="5" FontWeight="Bold" />

                                                        <Button x:Name="btnClear" Content="Clear" Grid.Column="13" Click="btnClear_Click" Margin="0,0,0,0"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.1*"/>
                                                            <ColumnDefinition Width="0.9*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.15*"/>
                                                            <ColumnDefinition Width="0.05*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock   Grid.Column="1" Text="系统时间：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxCurrent" Text="{Binding Current , StringFormat={}{0:yyyy-MM-dd HH:mm:ss}}" Grid.Column="2"  Margin="0,0,0,0"  Grid.ColumnSpan="2" IsReadOnly="True"/>

                                                        <TextBlock  Grid.Column="5" Text=" Device MAC：" Margin="0,3,3,0" HorizontalAlignment="Right"/>
                                                        <TextBox x:Name="tbxDeviceMac" Text="{Binding DeviceMacS}"   Grid.Column="6" Margin="0,0,5,0" IsReadOnly="True" MaxLines="1" MaxLength="11" FontWeight="Bold"/>
                                                        <TextBox x:Name="tbxNewDeviceMac" Text="" Grid.Column="7"  Margin="5,0,0,0" MaxLines="1" MaxLength="11" FontWeight="Bold" />

                                                        <TextBlock   Grid.Column="9" Text="硬件版本：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxHwRevision" Text="{Binding HwRevisionS}" Margin="0,0,5,0" Grid.Column="10" IsReadOnly="True" MaxLines="1" MaxLength="11" FontWeight="Bold"/>
                                                        <TextBox x:Name="tbxNewHwRevision" Text="" Margin="5,0,0,0" Grid.Column="11" MaxLength="11" MaxLines="1" FontWeight="Bold" />

                                                        <Button x:Name="btnUpdateFactory" Content="修改出厂配置" Grid.Column="13"  Click="btnUpdateFactory_Click"    Margin="0,0,0,0" Height="22" VerticalAlignment="Top"/>
                                                        <Button x:Name="btnHideFactory" Content="" Grid.Column="15" Click="btnHideFactory_Click" Margin="0,0,0,0"  Height="22" VerticalAlignment="Top" Foreground="White" BorderBrush="White" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}" BorderThickness="0"/>
                                                    </Grid>

                                                    <Grid Margin="0 15 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock  Grid.Column="1" Text=" 客户码：" Margin="0,3,3,0" HorizontalAlignment="Right"/>
                                                        <TextBox x:Name="tbxCustomer" Text="{Binding CustomerS}"   Grid.Column="2" Margin="0,0,5,0" IsReadOnly="True" MaxLines="1" MaxLength="5" FontWeight="Bold"/>
                                                        <TextBox x:Name="tbxNewCustomer" Text=""  Grid.Column="3"  Margin="5,0,0,0" MaxLength="5" MaxLines="1" FontWeight="Bold" />

                                                        <TextBlock   Grid.Column="5" Text="Debug：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxDebug" Text="{Binding DebugS}" Margin="0,0,5,0" Grid.Column="6" IsReadOnly="True" MaxLength="5" MaxLines="1" FontWeight="Bold"/>
                                                        <TextBox x:Name="tbxNewDebug" Text=""  Margin="5,0,0,0" Grid.Column="7" MaxLines="1" MaxLength="5" FontWeight="Bold" />

                                                        <TextBlock  Grid.Column="9" Text=" 分类码：" Margin="0,3,3,0" HorizontalAlignment="Right"/>
                                                        <TextBox x:Name="tbxCategory" Text="{Binding Category}"  Grid.Column="10"  Margin="0,0,5,0" IsReadOnly="True" MaxLength="3" MaxLines="1" />
                                                        <TextBox x:Name="tbxNewCategory" Text=""   Grid.Column="11"  Margin="5,0,0,0" MaxLength="3" MaxLines="1"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock  Grid.Column="1" Text="工作模式：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxPattern" Text="{Binding Pattern}"  Grid.Column="2"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewPattern" Text="" Grid.Column="3"  Margin="5 0 0 0" MaxLines="1" MaxLength="3"/>

                                                        <TextBlock   Grid.Column="5" Text="传输速率：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxBps" Text="{Binding bps}"  Grid.Column="6"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewBps" Text="" Grid.Column="7"  Margin="5 0 0 0" MaxLength="3" MaxLines="1"/>

                                                        <TextBlock   Grid.Column="9" Text="频段：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxChannel" Text="{Binding channel}" Grid.Column="10"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewChannel" Text="" Grid.Column="11" Margin="5 0 0 0" MaxLines="1" MaxLength="3"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock   Grid.Column="1" Text="心跳间隔：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxInterval" Text="{Binding Interval}"  Grid.Column="2" Margin="0 0 5 0" IsReadOnly="True" MaxLength="5" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewInterval" Text=""   Grid.Column="3"  Margin="5 0 0 0" MaxLength="5" MaxLines="1"/>

                                                        <TextBlock   Grid.Column="5" Text="轮播间隔：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxCarousel" Text="{Binding Carousel}"  Grid.Column="6"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="5" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewCarousel" Text=""  Grid.Column="7"  Margin="5 0 0 0" MaxLength="5" MaxLines="1"/>

                                                        <TextBlock   Grid.Column="9" Text="上传间隔：" Margin="0,3,3,0" HorizontalAlignment="Right" ToolTip="整分整时"  />
                                                        <TextBox x:Name="tbxIntervalOfAlert" Text="{Binding IntervalOfAlert}"  Grid.Column="10" Margin="0 0 5 0" IsReadOnly="True" MaxLength="5" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewIntervalOfAlert" Text="" Grid.Column="11" Margin="5 0 0 0" MaxLength="5" MaxLines="1"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock   Grid.Column="1" Text="转发策略：" Margin="0,3,3,0" HorizontalAlignment="Right" ToolTip="0=转发全部；1=转发绑定；" />
                                                        <TextBox x:Name="tbxTransPolicy" Text="{Binding TransPolicy}" Grid.Column="2"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewTransPolicy" Text="" Grid.Column="3"  Margin="5 0 0 0" MaxLength="3" MaxLines="1"/>

                                                        <TextBlock   Grid.Column="5" Text="显示时间：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxTimeSrc" Text="{Binding TimeSrc}"  Grid.Column="6"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewTimeSrc" Text=""  Grid.Column="7"  Margin="5 0 0 0"/>

                                                        <TextBlock   Grid.Column="9" Text="报警方式：" Margin="0,3,3,0" HorizontalAlignment="Right" ToolTip="0=静音；1=声报警；2=光报警；3=声光报警；" />
                                                        <TextBox x:Name="tbxAlertWay" Text="{Binding AlertWay}" Grid.Column="10"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewAlertWay" Text="" Grid.Column="11" Margin="5 0 0 0" MaxLength="3" MaxLines="1"/>

                                                        <Button x:Name="btnUpdateGateway" Content="修改应用配置" Grid.Column="13" Click="btnUpdateGateway_Click" Margin="0,0,0,0"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock   Grid.Column="1" Text="报警依据：" Margin="0,3,3,0" HorizontalAlignment="Right" ToolTip="0=关闭报警；1=依据传感器；2=依据本地；3=智能报警；" />
                                                        <TextBox x:Name="tbxCriSrc" Text="{Binding criSrc}"  Grid.Column="2"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewCriSrc" Text=""  Grid.Column="3" Padding="2" Margin="5 0 0 0" MaxLength="3" MaxLines="1" />

                                                        <TextBlock   Grid.Column="5" Text="显示方式：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxDisplayStyle" Text="{Binding DisplayStyle}"  Grid.Column="6"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewDisplayStyle" Text="" Grid.Column="7"  Margin="5 0 0 0" MaxLength="3" MaxLines="1"/>

                                                        <TextBlock   Grid.Column="9" Text="背光策略：" Margin="0,3,3,0" HorizontalAlignment="Right" ToolTip="0=自动熄屏；1=常亮屏幕；2=接电常亮；" />
                                                        <TextBox x:Name="tbxBrightMode" Text="{Binding brightMode}"  Grid.Column="10"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="3" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewBrightMode" Text=""  Grid.Column="11"  Margin="5 0 0 0" MaxLength="3" MaxLines="1"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock   Grid.Column="1" Text="启用功能：" Margin="0,3,3,0" HorizontalAlignment="Right" ToolTip="bit3=运动检测；bit2=CC1101；bit1=BLE；bit0=GPS；" />
                                                        <TextBox x:Name="tbxUse" Text="{Binding use, StringFormat=\{0:X2\}}"  Grid.Column="2"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="2" MaxLines="1" FontWeight="Bold"/>
                                                        <TextBox x:Name="tbxNewUse" Text="" Grid.Column="3"  Margin="5 0 0 0" MaxLength="2" MaxLines="1" FontWeight="Bold"/>

                                                        <TextBlock   Grid.Column="5" Text="定位间隔：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxIntervalOfLocate" Text="{Binding IntervalOfLocate}"  Grid.Column="6"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="5" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewIntervalOfLocate" Text=""  Grid.Column="7" Padding="2" Margin="5 0 0 0" MaxLength="5" MaxLines="1"/>

                                                        <TextBlock   Grid.Column="9" Text="射频功率：" Margin="0,3,3,0" HorizontalAlignment="Right" />
                                                        <TextBox x:Name="tbxTxPower" Text="{Binding TxPower}" Grid.Column="10"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="4" MaxLines="1"/>
                                                        <TextBox x:Name="tbxNewTxPower" Text="" Grid.Column="11"  Margin="5 0 0 0" MaxLength="4" MaxLines="1"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="1.6*"/>
                                                            <ColumnDefinition Width="0.2*"/>
                                                        </Grid.ColumnDefinitions>

                                                        <TextBlock   Grid.Column="1" Text="配置保留位：" Margin="0,3,3,0" HorizontalAlignment="Right" ToolTip="byte0=工作场景；" />
                                                        <TextBox x:Name="tbxReserved" Text="{Binding ReservedS}" Grid.Column="2"  Margin="0 0 5 0" IsReadOnly="True" MaxLength="8" MaxLines="1" FontWeight="Bold"/>
                                                        <TextBox x:Name="tbxNewReserved" Text="" Grid.Column="3"  Margin="5 0 0 0" MaxLength="8" MaxLines="1" FontWeight="Bold"/>
                                                    </Grid>

                                                    <Grid Margin="0 5 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.2*"/>
                                                            <ColumnDefinition Width="0.8*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
